{
  "name": "@reliverse/runtime",
  "version": "1.0.3",
  "description": "@reliverse/runtime is a library that detects the environment runtime, installed package managers, identifies the correct package manager, and provides various useful utilities for developers and users.",
  "type": "module",
  "scripts": {
    "pub": "relidler",
    "dev": "bun examples/e-main.ts",
    "latest": "bun update --latest && bun check",
    "check": "tsc --noEmit && eslint --cache --fix . && biome check --fix --unsafe . && knip",
    "vi": "vitest"
  },
  "dependencies": {
    "execa": "^9.5.2",
    "fs-extra": "^11.3.0",
    "pathe": "^2.0.3"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@eslint/js": "^9.23.0",
    "@reliverse/relidler-cfg": "^1.1.3",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@types/bun": "^1.2.8",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.14",
    "eslint": "^9.23.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-perfectionist": "^4.10.1",
    "knip": "^5.46.4",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0",
    "vitest": "^3.1.1"
  }
}
